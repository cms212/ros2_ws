<?xml version="1.0" ?>
<sdf version="1.10">
    <model name='robot' canonical_link='base_link'>
      <link name='base_link'>
        <pose relative_to='__model__'>0 0 0 0 0 0</pose>
	<inertial> <!--inertial properties of the link mass, inertia matix-->
        <mass>0.00001</mass>
        <inertia>
            <ixx>0.0009</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.0009</iyy>
            <iyz>0</iyz>
            <izz>0.0009</izz>
        </inertia>
	</inertial>

    </link>
    <link name='base_footprint'>
        <pose relative_to="base_link">0 0 0 0 0 0</pose>
    </link>
    <joint name='base_footprint_joint' type='fixed'>
        <parent>base_link</parent>
        <child>base_footprint</child>
    </joint>

    <link name='chassis'>
        <pose relative_to='base_link'>0 0 0.2 0 0 0</pose>
     <inertial> <!--inertial properties of the link mass, inertia matix-->
        <mass>2.28</mass>
        <inertia>
            <ixx>0.03667</ixx>
            <iyy>0.02383</iyy>
            <izz>0.045833</izz>
        </inertia>
    </inertial>
    <visual name='visual_chassis'>
        <geometry>
          <box>
            <size>0.568 0.296 0.193</size>
          </box>
        </geometry>
        <material>
            <ambient>0.0 0.0 1.0 1</ambient>
            <diffuse>0.0 0.0 1.0 1</diffuse>
            <specular>0.0 0.0 1.0 1</specular>
        </material>
      </visual>
      <collision name='collision_chassis'>
        <geometry>
          <box>
            <size>0.568 0.296 0.193</size>
          </box>
        </geometry>
      </collision>
    </link>
    <joint name='chassis_joint' type='fixed'>
	    <parent>base_link</parent>
      <child>chassis</child>
    </joint>
    
    <link name='left_wheel'>
    <pose relative_to="chassis">-0.25 0.19 -0.02 -1.5707 0 0</pose>
    <inertial>
        <mass>0.1</mass>
        <inertia>
            <ixx>0.000233</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.000233</iyy>
            <iyz>0</iyz>
            <izz>0.000405</izz>
        </inertia>
      </inertial>
    <visual name='visual'>
      <pose>0 0 0 -1.5707 0 0</pose>
      <geometry>
	 <mesh>
	 <uri>package://robot/models/robot/meshes/wheel.dae</uri>
	 <scale>0.5 0.5 0.5</scale>
	 </mesh>
      </geometry>
    </visual>
    <collision name='collision'>
      <geometry>
        <cylinder>
           <radius>0.09</radius>
           <length>0.06</length>
        </cylinder>
      </geometry>
    </collision>
</link>
<!--The same as left wheel but with different position-->
<link name='right_wheel'>
    <pose relative_to="chassis">-0.25 -0.19  -0.02 -1.5707 0 0</pose> <!--angles are in radian-->
    <inertial>
        <mass>0.1</mass>
        <inertia>
            <ixx>0.000233</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.000233</iyy>
            <iyz>0</iyz>
            <izz>0.000405</izz>
        </inertia>
    </inertial>
    <visual name='visual'>
      <pose>0 0 0 -1.5707 0 0</pose>
      <geometry>
         <mesh>

	 <uri>package://robot/models/robot/meshes/wheel.dae</uri>
	    <!--<uri>meshes/wheel.dae</uri>-->
            <scale>0.5 0.5 0.5</scale>
         </mesh>
      </geometry>
    </visual>
    <collision name='collision'>
      <geometry>
        <cylinder>
           <radius>0.09</radius>
           <length>0.06</length>
        </cylinder>
      </geometry>
    </collision>
</link>
<frame name="caster_frame" attached_to='chassis'>
    <pose>0.25 0 -0.1 0 0 0</pose>
</frame>
<!--caster wheel-->
<link name='caster'>
    <pose relative_to='caster_frame'/>
    <inertial>
        <mass>0.1</mass>
        <inertia>
            <ixx>0.000004</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.000004</iyy>
            <iyz>0</iyz>
            <izz>0.000004</izz>
        </inertia>
      </inertial>
      <collision name='collision'>
      <visual>
        <sphere>
           <radius>0.02</radius>
        </sphere>
      </visual>
      <geometry>
        <sphere>
           <radius>0.02</radius>
        </sphere>
      </geometry>
    </collision>
  </link>
    <joint name='left_wheel_joint' type='revolute'>
      <parent>chassis</parent>
      <child>left_wheel</child>
      <axis>
        <xyz expressed_in='__model__'>0 1 0</xyz> 
        <limit>
          <lower>-1.79769e+308</lower>    <!--negative infinity-->
          <upper>1.79769e+308</upper>     <!--positive infinity-->
        </limit>
      </axis>
    </joint>
    <joint name='right_wheel_joint' type='revolute'>
      <parent>chassis</parent>
      <child>right_wheel</child>
      <axis>
        <xyz expressed_in='__model__'>0 1 0</xyz>
        <limit>
          <lower>-1.79769e+308</lower>    <!--negative infinity-->
          <upper>1.79769e+308</upper>     <!--positive infinity-->
        </limit>
    </axis>
</joint>
<joint name='caster_wheel' type='ball'>
    <parent>chassis</parent>
    <child>caster</child>
</joint>
<plugin
    filename="gz-sim-diff-drive-system"
    name="gz::sim::systems::DiffDrive">
    <left_joint>left_wheel_joint</left_joint>
    <right_joint>right_wheel_joint</right_joint>
    <wheel_separation>0.3</wheel_separation>
    <wheel_radius>0.09</wheel_radius>
    <topic>cmd_vel</topic>
    <odom_publish_frequency>30</odom_publish_frequency>
    <odom_topic>odom</odom_topic>
    <tf_topic>tf</tf_topic>
    <frame_id>odom</frame_id>
    <child_frame_id>base_link</child_frame_id>
</plugin>

 <link name='camera_body'>
     <pose relative_to="chassis">0.286 0 0 0 0 0</pose>
     <inertial> <!--inertial properties of the link mass, inertia matix-->
        <mass>0.005</mass>
        <inertia>
            <ixx>0.0000001</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.0000001</iyy>
            <iyz>0</iyz>
            <izz>0.0000001</izz>
        </inertia>
     </inertial>
            
            <visual name='visual'>
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                    <cylinder>
                        <radius>0.01</radius>
                        <length>0.01</length>
                    </cylinder>
                </geometry>
                <material>
                    <ambient>0 0.1 0 0.1</ambient>
                    <diffuse>0 0.2 0 1</diffuse>
                    <specular>0 0 0.5 1</specular>
                    <emissive>0 0 0.3 1</emissive>
                </material>
            </visual>
            <collision name='collision'>
                <pose>0 0 0 0 1.5707 0</pose>
                <geometry>
                    <cylinder>
                        <radius>0.01</radius>
                        <length>0.01</length>
                    </cylinder>
                </geometry>
            </collision>
            <sensor name="camera" type="camera">
              <camera>
                <pose>0 0 0 0 0 0</pose>
                <horizontal_fov>1.047</horizontal_fov>
                <image>
                  <width>800</width>
                  <height>600</height>
                </image>
                <clip>
                  <near>0.1</near>
                  <far>100</far>
                </clip>
                <optical_frame_id>camera</optical_frame_id>
              </camera>
              <always_on>1</always_on>
              <update_rate>30</update_rate>
              <visualize>true</visualize>
              <topic>camera</topic>
            </sensor>
        </link>
        <link name='camera'>
        <inertial>
          <mass>0.01</mass>
          <inertia>
            <ixx>0.0000001</ixx>
            <iyy>0.0000001</iyy>
            <izz>0.0000001</izz>
          </inertia>
        </inertial>
            <pose relative_to="camera_body">0.01 0 0 1.5708 3.141 1.5708</pose>
        </link>
        <joint name='camera_joint' type='fixed'>
            <parent>chassis</parent>
            <child>camera_body</child>
        </joint>
        <joint name='camera_optical_joint' type='fixed'>
            <parent>camera_body</parent>
            <child>camera</child>
        </joint>
     <link name="lidar">
        <pose relative_to="chassis">0.1 0.0 0.1 0 0 0</pose>
        <inertial>
          <mass>0.01</mass>
          <inertia>
          <inertia>
            <ixx>0.0000001</ixx>
            <iyy>0.0000001</iyy>
            <izz>0.0000001</izz>
          </inertia>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <box>
              <size>0.01 0.01 0.01</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.01 0.01 0.01</size>
            </box>
          </geometry>
        </visual>

        <sensor name='gpu_lidar' type='gpu_lidar'>
          <pose relative_to='lidar'>0 0 0.05 0 0 0</pose>
          <topic>scan</topic>
          <gz_frame_id>lidar</gz_frame_id>
          <update_rate>10</update_rate>
          <lidar>
            <scan>
              <horizontal>
                <samples>1081</samples>
                <resolution>1</resolution>
                <min_angle>-2.356</min_angle>
                <max_angle>2.356</max_angle>
              </horizontal>
            </scan>
            <range>
              <min>0.08</min>
              <max>10.0</max>
              <resolution>0.01</resolution>
            </range>
          </lidar>
          <alwaysOn>1</alwaysOn>
          <visualize>true</visualize>
        </sensor>
      </link>
      <joint name='lidar_joint' type='fixed'>
	 <parent>chassis</parent>
         <child>lidar</child>
      </joint>
      <link name='imu'>
            <pose relative_to="chassis">0.1 0.075 0.1 0 0 0</pose>
     <inertial> <!--inertial properties of the link mass, inertia matix-->
        <mass>0.005</mass>
        <inertia>
          <inertia>
            <ixx>0.0000001</ixx>
            <iyy>0.0000001</iyy>
            <izz>0.0000001</izz>
          </inertia>
        </inertia>
</inertial>
            
            <visual name='visual'>
                <geometry>
                    <box>
                        <size>0.015 0.015 0.003</size>
                    </box>
                </geometry>
                <material>
                    <ambient>0 0 0.1 0.1</ambient>
                    <diffuse>0 0 0.2 1</diffuse>
                    <specular>0 0 0.5 1</specular>
                    <emissive>0 0 0.3 1</emissive>
                </material>
            </visual>
            <collision name='collision'>
                <geometry>
                    <box>
                        <size>0.015 0.015 0.003</size>
                    </box>
                </geometry>
            </collision>
            <sensor name="imu_sensor" type="imu">
                <always_on>1</always_on>
                <update_rate>30</update_rate>
                <visualize>true</visualize>
                <topic>imu</topic>
                <gz_frame_id>imu</gz_frame_id>
            </sensor>
        </link>
        <joint name='imu_joint' type='fixed'>
            <parent>chassis</parent>
            <child>imu</child>
        </joint>

     <link name='gps'>
        <pose relative_to="chassis">0.1 -0.075 0.1 0 0 0</pose>
        <inertial> <!--inertial properties of the link mass, inertia matix-->
          <mass>0.005</mass>
           <inertia>
          <inertia>
            <ixx>0.0000001</ixx>
            <iyy>0.0000001</iyy>
            <izz>0.0000001</izz>
          </inertia>
          </inertia>
        </inertial>
            <visual name='visual'>
                <geometry>
                    <box>
                        <size>0.015 0.015 0.003</size>
                    </box>
                </geometry>
                <material>
                    <ambient>0 0 0 0.1</ambient>
                    <diffuse>0 0 0 1</diffuse>
                    <specular>0 0 0 1</specular>
                    <emissive>0 0 0 1</emissive>
                </material>
            </visual>
            <collision name='collision'>
                <geometry>
                    <box>
                        <size>0.015 0.015 0.003</size>
                    </box>
                </geometry>
            </collision>
            <sensor name="navsat_sensor" type="navsat">
              <always_on>1</always_on>
              <update_rate>30</update_rate>
              <topic>navsat</topic>
              <gz_frame_id>gps</gz_frame_id>
            </sensor>
        </link>
        <joint name='gps_joint' type='fixed'>
            <parent>chassis</parent>
            <child>gps</child>
        </joint>
      <plugin
        filename="gz-sim-joint-state-publisher-system"
        name="gz::sim::systems::JointStatePublisher">
      </plugin>

      <plugin
        filename="gz-sim-pose-publisher-system"
        name="gz::sim::systems::PosePublisher">
        <publish_link_pose>true</publish_link_pose>
        <use_pose_vector_msg>true</use_pose_vector_msg>
        <static_publisher>true</static_publisher>
        <static_update_frequency>1</static_update_frequency>
      </plugin>


</model>
</sdf>
